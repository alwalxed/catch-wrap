{
  "name": "catch-wrap",
  "version": "1.0.1",
  "description": "Type-safe try-catch utility for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "error-handling",
    "try-catch",
    "promise",
    "async",
    "utilities",
    "type-safe",
    "result",
    "maybe",
    "either"
  ],
  "author": "Awa <alwalxed@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alwalxed/catch-wrap.git"
  },
  "bugs": {
    "url": "https://github.com/alwalxed/catch-wrap/issues"
  },
  "homepage": "https://github.com/alwalxed/catch-wrap#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
